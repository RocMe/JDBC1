<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>login_form</servlet-name>
        <servlet-class>com.action.login_form</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register_form</servlet-name>
        <servlet-class>com.action.register_form</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>query_form</servlet-name>
        <servlet-class>com.action.query_form</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>insert_form</servlet-name>
        <servlet-class>com.action.insert_form</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update_form</servlet-name>
        <servlet-class>com.action.update_form</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>user_data_form</servlet-name>
        <servlet-class>com.action.user_data_form</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>upload_form</servlet-name>
        <servlet-class>com.action.upload_form</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sysdba_form</servlet-name>
        <servlet-class>com.action.sysdba_form</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>contact_us_form</servlet-name>
        <servlet-class>com.action.contact_us_form</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login_form</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register_form</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>query_form</servlet-name>
        <url-pattern>/query</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>insert_form</servlet-name>
        <url-pattern>/insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update_form</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>user_data_form</servlet-name>
        <url-pattern>/user_data</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>upload_form</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sysdba_form</servlet-name>
        <url-pattern>/sysdba</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>contact_us_form</servlet-name>
        <url-pattern>/contact_us</url-pattern>
    </servlet-mapping>
</web-app>